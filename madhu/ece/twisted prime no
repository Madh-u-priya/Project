for(int i=2;i<a;i++)
		{
			if(a%i==0)
				c++;
		}
		if(c==0)
		{
		while(a!=0)
		{
			int r=a%10;
			reverse=reverse*10+r;
			a/=10;
		}
			for(int i=2;i<reverse;i++)
			{
				if(reverse%i==0)
					c++;
			}
			if(c==0) {
				System.out.println("Twisted Prime Number");
			}
			else {
		System.out.println("Not Twisted Prime Number");
			}
		}
		else
		{
			System.out.println("Not Twisted Prime Number");
		}

}
}
